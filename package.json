{
  "name": "electron-react-webpack",
  "productName": "Electron React Webpack",
  "description": "Electron + React + Webpack + TypeScript boilerplate",
  "version": "1.0.0",
  "appId": "com.company.app-name",
  "author": "IRONAGE-Park",
  "license": "ISC",
  "main": "./build/main.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "private": true,
  "homepage": "https://github.com/IRONAGE-Park/electron-react-wepback#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IRONAGE-Park/electron-react-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/IRONAGE-Park/electron-react-webpack/issues"
  },
  "scripts": {
    "start:main": "node scripts/start-main.js",
    "build:main": "node scripts/build-main.js",
    "start:renderer": "node scripts/start-renderer.js",
    "build:renderer": "node scripts/build-renderer.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "test:clear": "jest --cacheClear",
    "build": "rm -rf ./build && yarn build:renderer && yarn build:main",
    "package": "rm -rf ./dist && yarn build && node scripts/package.js",
    "package:no-sign": "yarn package --no-sign",
    "package:no-asar": "yarn package:no-sign --no-asar",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "postinstall": "electron-rebuild"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^7.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.2",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-resolve": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^5.0.0",
    "semver": "^7.3.5",
    "source-map-loader": "^4.0.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "web-vitals": "^3.3.0",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.4.1"
  }
}
